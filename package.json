{
  "name": "qwind",
  "description": "A template to make your website using Qwik + Tailwind CSS.",
  "version": "0.121.0",
  "type": "module",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "trustedDependencies": [
    "sharp"
  ],
  "trustedDependencies-annotation": "Needed for bun to allow running install scripts",
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/vercel-edge/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "vercel deploy",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "serve": "node server/entry.express",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.0.18",
    "@modular-forms/qwik": "^0.26.1",
    "@rimbu/core": "^2.0.3",
    "@unpic/qwik": "^0.0.46",
    "@upstash/redis": "^1.33.0",
    "gray-matter": "^4.0.3",
    "markdown-it": "^14.1.0",
    "node-fetch": "3.3.2",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "tailwind-merge": "^2.3.0",
    "valibot": "^0.36.0",
    "xstate": "^5.16.0"
  },
  "devDependencies": {
    "@auth/qwik": "0.2.2",
    "@builder.io/qwik": "^1.5.6",
    "@builder.io/qwik-city": "^1.5.6",
    "@divriots/jampack": "^0.24.4",
    "@tailwindcss/typography": "^0.5.13",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.56.10",
    "@types/express": "^4.17.19",
    "@types/markdown-it": "^14.1.1",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "autoprefixer": "^10.4.19",
    "compression": "^1.7.4",
    "dotenv": "^16.3.2",
    "eslint": "^8.57.0",
    "eslint-plugin-qwik": "^1.5.6",
    "postcss": "^8.4.38",
    "prettier": "^3.3.1",
    "prettier-plugin-tailwindcss": "^0.6.2",
    "tailwindcss": "3.4.4",
    "typescript": "5.4.5",
    "vercel": "^29.1.1",
    "vite": "^5.2.13",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
